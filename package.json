{
  "name": "injecd",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/my-lib.umd.cjs",
  "module": "./dist/my-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "description": "JS/TS injection that minimizes boilerplate by leveraging default parameters",
  "author": "Jakub Kolezynski",
  "license": "MIT",
  "homepage": "https://kolezyn.ski/",
  "repository": {
    "type": "git",
    "url": "https://github.com/JakeBeaver/injecd.git"
  },
  "bugs": {
    "url": "https://github.com/JakeBeaver/injecd/issues"
  },
  "keywords": [
    "ioc",
    "dependency",
    "injection",
    "container"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "lint": "prettier --write . && eslint",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.27.1"
  },
  "dependencies": {
    "prettier": "^2.8.3"
  }
}
